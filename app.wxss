/**app.wxss**/

page {
  height: 100%;
  background: #f5f5f5;
  font-size: 11pt;
  font-family: "微软雅黑", Arial, Helvetica, sans-serif;
}

.container {
  display: flex;
  font-size: 14px;
  flex-direction: column;
  font-family: "微软雅黑", Arial, Helvetica, sans-serif;
}

.clearfix:before, .clearfix:after {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.placeholer {
  color: #bcbcbc;
}
